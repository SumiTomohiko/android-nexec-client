<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.gr.java_conf.neko_daisuki.android.nexec.client"
    android:versionCode="1"
    android:versionName="1.0">
